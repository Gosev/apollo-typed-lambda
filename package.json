{
  "name": "@tomyitav/apollo-typed-lambda",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-schema": "rimraf ./dist/graphql/schema/ && dir /s /b \"src/graphql/types\" | findstr /e .ts > ts-files.txt && tsc ./src/graphql/schema/genereator-schema.ts --outDir ./dist/graphql/schema/ --lib esnext && tsc @ts-files.txt --outDir ./dist/graphql/types --lib esnext && del ts-files.txt",
    "generate-ts": "gql-gen",
    "pregenerate": "npm run build-schema",
    "generate": "npm run generate-ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run generate",
    "build": "tsc",
    "start": "sls offline start --skipCacheInvalidation",
    "dev": "tsc --watch & nodemon dist",
    "test": "tsc && mocha dist/**/*.spec.js",
    "lint": "eslint src --ext ts",
    "tsc": "tsc",
    "sls:offline": "npm run build && npm start",
    "deploy": "npm run build && sls deploy"
  },
  "_moduleAliases": {
    "@src": "dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server-lambda": "2.3.1",
    "core-js": "2.6.2",
    "graphql": "14.1.1",
    "graphql-tools": "4.0.3",
    "injection-js": "2.2.1",
    "merge-graphql-schemas": "1.5.8",
    "module-alias": "2.1.0",
    "reflect-metadata": "0.1.13",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@types/graphql": "14.0.5",
    "@types/node": "10.12.18",
    "graphql-code-generator": "0.16.0",
    "graphql-codegen-typescript-client": "0.16.0",
    "graphql-codegen-typescript-common": "0.16.0",
    "graphql-codegen-typescript-server": "0.16.0",
    "rimraf": "2.6.3",
    "serverless-dotenv-plugin": "2.0.1",
    "serverless-offline": "4.1.4",
    "typescript": "3.2.4"
  }
}
